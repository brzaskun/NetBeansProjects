								<xsd:element name="P_11" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Liczba podatników za m-c II</xsd:documentation>
									</xsd:annotation>
									<xsd:simpleType>
										<xsd:restriction base="etd:TNaturalny">
											<xsd:minInclusive value="0"/>
										</xsd:restriction>
